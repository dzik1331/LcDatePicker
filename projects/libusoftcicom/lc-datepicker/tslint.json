{
  "extends": "../../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "lib",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "lib",
      "kebab-case"
    ],
    "no-inferrable-types": false,
    "prefer-const": false,
    "no-angle-bracket-type-assertion": false,
    "no-string-throw": false,
    "no-redundant-jsdoc": false
  }
}
